#include <cstdio>

int binary_search(int *numbers, int n, int val) {
	int left = 0, right = n - 1;
	while(left < right) {
		int middle = (left + right) / 2;
		if (numbers[middle] < val) {
			left = middle + 1;
		} else {
			right = middle - 1;
		}
	}

	return right;
}

int main() {
	int array[] = {6, 13, 14, 25, 33, 43, 51, 53, 64, 72, 84, 93, 95, 96, 97};
 	printf("%d\n", binary_search(array, 15, 35));    // 10 50 60 
	printf("%d\n", binary_search(array, 15, 33));
printf("%d\n", binary_search(array, 15, 25));
printf("%d\n", binary_search(array, 15, 6));
	return 0;
} 
